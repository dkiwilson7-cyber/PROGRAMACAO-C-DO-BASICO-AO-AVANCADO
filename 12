/*  Faça um programa, no qual  receba e some 5 números inteiros.  */



#include <stdio.h>
#include <stdlib.h>

int main()
{
    int numero; // armazena o número digitado
    int soma = 0; // acumuladora

    for(int i = 0; i < 5; i++)
    {
        printf("Digite um numero inteiro: \n");
        scanf("%d", &numero);

        soma += numero;
    }

    printf("A soma dos numeros inteiros eh: %d\n", soma);


    system("pause");
    return 0;
}

